{
    "name": "producer/githooks",
    "description": "Git hook scripts via composer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul M. Jones",
            "email": "pmjones@pmjones.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Producer\\Githooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Producer\\Githooks\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "pds/skeleton": "~1.0"
    },
    "scripts": {
        "post-package-install": [
            "Producer\\Githooks\\Composer::postPackageInstall"
        ],
        "post-install": [
            "Producer\\Githooks\\Composer::postInstall"
        ],
        "post-update": [
            "Producer\\Githooks\\Composer::postUpdate"
        ]
    },
    "minimum-stability": "dev"
}
